    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^13.1.0",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-watch": "^4.3.1",
    "simple-mock": "^0.8.0",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.2",
    "tslib": "^1.9.3",
    "typescript": "^3.2.4"
