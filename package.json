{
  "name": "orcajs",
  "version": "1.1.5",
  "main": "dist/orca.cjs.js",
  "module": "dist/orca.esm.js",
  "browser": "dist/orca.umd.js",
  "license": "MIT",
  "description": "orchestrate your javascript",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "test": "nyc mocha test/**/*.ts",
    "precoveralls": "npm test",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*.{ts,tsx}"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.120",
    "@types/mocha": "^5.2.5",
    "coveralls": "^3.0.2",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.16.1",
    "pretty-quick": "^1.10.0",
    "rollup": "^1.1.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-watch": "^4.3.1",
    "simple-mock": "^0.8.0",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.2",
    "tslib": "^1.9.3",
    "typescript": "^3.2.4"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm test"
    }
  }
}
